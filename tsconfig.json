{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"strictNullChecks": true,
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"src/*"
			],
			"@components/*": [
				"src/components/*"
			],
			"@assets/*": [
				"src/assets/*"
			],
			"@modules/*": [
				"src/modules/*"
			],
			"@libs/*": [
				"src/libs/*"
			],
			"@apps/*": [
				"src/apps/*"
			],
			"@types/*": [
				"src/types/*"
			],
			"@mocks/*": [
				"src/mocks/*"
			],
			"@schemas/*": [
				"src/schemas/*"
			],
			"@layouts/*": [
				"src/layouts/*"
			],
			"@utils/*": [
				"src/utils/*"
			],
			"@react/*": [
				"src/shared/React/*"
			],
			"@vue/*": [
				"src/shared/Vue/*"
			]
		}
	}
}